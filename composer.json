{
    "config": {
        "preferred-install": "dist",
        "platform": {
            "php": "5.6"
        },
        "optimize-autoloader": true,
        "prepend-autoloader": false
    },
    "require-dev": {
        "php": ">=5.6",
        "phpunit/phpunit": "~5.7",
        "guzzlehttp/guzzle": "~5.0",
        "prestashop/php-coding-standards": "dev-add-header-stamp"
    },
    "autoload": {
        "psr-4": {
            "PrestaShop\\Module\\PrestashopCheckout\\": "classes/"
        },
        "classmap": [
            "ps_checkout.php",
            "controllers/"
        ]
    },
    "require": {
        "ramsey/uuid": "^3.8",
        "vlucas/phpdotenv": "^3.4"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Quetzacoalt91/php-coding-standards"
        }
    ],
    "author": "PrestaShop",
    "license": "AFL-3.0"
}