includes:
	- %currentWorkingDirectory%/vendor/prestashop/php-dev-tools/phpstan/ps-module-extension.neon

parameters:
	paths:
		# From PHPStan 0.12, paths to check are relative to the neon file
		- ../../classes
		- ../../controllers
		- ../../ps_checkout.php
	ignoreErrors:
		- '#Method LinkCore::getAdminLink\(\) invoked with 4 parameters, 1-2 required.#'
		- '#Call to method assign\(\) on an unknown class Smarty.#'
		- '#Instantiated class PrestaShop\\PrestaShop\\Adapter\\Cart\\CartPresenter not found#'
		- '#Call to method present\(\) on an unknown class PrestaShop\\PrestaShop\\Adapter\\Cart\\CartPresenter.#'
		- '#Property ModuleCore::\$version \(float\) does not accept string.#'
		- '#Parameter #1 \$hook_name of method ModuleCore::registerHook\(\) expects string, array<int, string> given.#'
		- '#Property TabCore::\$name \(string\) does not accept array.#'
		- '#Class CheckoutPaymentStep not found.#'
		- '#Call to method isCurrent() on an unknown class CheckoutPaymentStep.#'
		- '#Instantiated class PrestaShop\\PrestaShop\\Core\\Payment\\PaymentOption not found.#'

	level: 5
